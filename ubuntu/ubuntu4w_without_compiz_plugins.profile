[animation]
s0_unminimize_effects = animation:Glide 2;

[expo]
s0_expo_key = Disabled
s0_expo_edge = TopRight
s0_x_offset = 48
s0_distance = 0.005000
s0_vp_saturation = 40.000000
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[core]
s0_active_plugins = core;composite;opengl;move;regex;wall;compiztoolbox;animation;place;snap;fade;session;resize;mousepoll;imgpng;vpswitch;scale;unitymtgrabhandles;copytex;workarounds;commands;grid;expo;ezoom;unityshell;
s0_outputs = 1366x768+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_show_desktop_key = <Control><Super>d
s0_hsize = 4

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[gnomecompat]
s0_main_menu_key = <Super>s
s0_command_window_screenshot = gnome-screenshot -w
s0_command_terminal = x-terminal-emulator
s0_run_command_terminal_key = <Control><Alt>t

[place]
s0_mode_matches = class=Gnome-terminal;
s0_mode_modes = 3;

[unityshell]
s0_show_menu_bar = Disabled
s0_lock_screen = <Control><Alt><Super>l
s0_show_hud = Disabled
s0_execute_command = Disabled
s0_show_desktop_key = Disabled
s0_panel_first_menu = Disabled
s0_shortcut_overlay = false
s0_inactive_shadow_color = #000000a5
s0_show_launcher = Disabled
s0_keyboard_focus = Disabled
s0_launcher_switcher_forward = <Super>r
s0_launcher_switcher_prev = <Shift><Super>r
s0_launcher_hide_mode = 1
s0_edge_responsiveness = 8.000000
s0_icon_size = 32
s0_alt_tab_prev = Disabled
s0_alt_tab_forward_all = Disabled
s0_alt_tab_prev_all = Disabled
s0_disable_show_desktop = true

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9
s0_allow_wraparound = true
s0_left_key = <Super>h
s0_right_key = <Super>l
s0_up_key = Disabled
s0_down_key = Disabled
s0_left_window_key = <Shift><Super>h
s0_right_window_key = <Shift><Super>l
s0_up_window_key = Disabled
s0_down_window_key = Disabled

[scale]
s0_x_offset = 48
s0_initiate_key = Disabled
s0_initiate_all_edge = TopLeft
s0_initiate_all_key = Disabled

[vpswitch]
s0_switch_to_1_key = <Super>1
s0_switch_to_2_key = <Super>2
s0_switch_to_3_key = <Super>3
s0_switch_to_4_key = <Super>9

[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

